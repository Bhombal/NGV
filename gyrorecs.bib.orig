@article{ ISI:A1993LB78400024,
Author = {KROMMES, JA},
Title = {{THERMAL FLUCTUATIONS IN GYROKINETIC PLASMA AT FINITE-BETA}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{1993}},
Volume = {{70}},
Number = {{20}},
Pages = {{3067-3070}},
Month = {{MAY 17}},
Abstract = {{A covariant gyrokinetic fluctuation-dissipation theorem is used to
   predict unusual thermal-equilibrium fluctuation spectra for weakly
   electromagnetic gyrokinetic plasma in the limit of weak coupling. Wave
   number spectra are derived both by exact integrations of the frequency
   spectra and by resonance integrations over weakly damped normal modes.
   The former reduce in the electrostatic limit to previous heuristic
   results and are interpreted in terms of the concept of a `'gyrokinetic
   vacuum.'' The latter are interpreted in terms of a new form for the
   gyrokinetic energy conservation law and a new formula for the
   gyrokinetic plasmon action density.}},
