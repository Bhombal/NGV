@article{ ISI:000281906300016,
Author = {Wang, Lu and Hahm, T. S.},
Title = {{Nonlinear gyrokinetic theory with polarization drift}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2010}},
Volume = {{17}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{A set of the electrostatic toroidal gyrokinetic Vlasov equation and the
   Poisson equation, which explicitly includes the polarization drift, is
   derived systematically by using Lie-transform perturbation method. The
   polarization drift is introduced in the gyrocenter equations of motion,
   and the corresponding polarization density is derived. Contrary to the
   widespread expectation, the inclusion of the polarization drift in the
   gyrocenter equations of motion does not affect the expression for the
   polarization density significantly. This is due to modification of the
   gyrocenter phase-space volume caused by the electrostatic potential
   {[}T. S. Hahm, Phys. Plasmas 3, 4658 (1996)]. (C) 2010 American
   Institute of Physics. {[}doi: 10.1063/1.3467498]}},
