@article{ ISI:000222653100003,
Author = {Lewandowski, JLV},
Title = {{Optimized loading for particle-in-cell gyrokinetic simulations}},
Journal = {{CANADIAN JOURNAL OF PHYSICS}},
Year = {{2004}},
Volume = {{82}},
Number = {{8}},
Pages = {{609-622}},
Month = {{AUG}},
Abstract = {{The problem of particle loading in particle-in-cell gyrokinetic
   simulations is addressed using a quadratic optimization algorithm.
   Optimized loading in configuration space dramatically reduces the
   short-wavelength modes in the electrostatic potential that are partly
   responsible for the nonconservation of total energy; further, the
   long-wavelength modes are resolved with good accuracy. As a result, the
   conservation of energy for the optimized loading is much better that the
   conservation of energy for the random loading. The method is valid for
   any geometry and can be coupled to optimization algorithms in velocity
   space.}},
