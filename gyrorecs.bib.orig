@article{ ISI:000630137500001,
Author = {Cole, M. D. J. and Mishchenko, A. and Bottino, A. and Chang, C. S.},
Title = {{Tokamak ITG-KBM transition benchmarking with the mixed
   variables/pullback transformation electromagnetic gyrokinetic scheme}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2021}},
Volume = {{28}},
Number = {{3}},
Month = {{FEB}},
Abstract = {{Electromagnetic gyrokinetic simulation of high temperature plasma is
   required to predict confinement in magnetic fusion devices and has posed
   challenges for existing codes. In this paper, we demonstrate successful
   global gyrokinetic simulation of the ion temperature gradient-driven
   mode-kinetic ballooning mode transition in a toroidal fusion plasma test
   case using the mixed variables/pullback transformation (MV/PT) scheme
   with the particle-in-cell codes XGC and ORB5, and compare to results
   from a conventional continuum code from the literature. The MV/PT scheme
   combines explicit time integration with mitigation of the well-known
   electromagnetic gyrokinetic ``cancelation problem.{''} We calculate
   eigenmodes in the electrostatic and parallel vector potentials, and find
   good agreement in growth rate, real frequency, and the normalized plasma
   pressure of mode transition.}},
