@article{ ISI:000334180200054,
Author = {Navarro, A. Banon and Teaca, B. and Jenko, F. and Hammett, G. W. and
   Happel, T. and ASDEX Upgrade Team},
Title = {{Applications of large eddy simulation methods to gyrokinetic turbulence}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2014}},
Volume = {{21}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{The large eddy simulation (LES) approach-solving numerically the large
   scales of a turbulent system and accounting for the small-scale
   influence through a model-is applied to nonlinear gyrokinetic systems
   that are driven by a number of different microinstabilities. Comparisons
   between modeled, lower resolution, and higher resolution simulations are
   performed for an experimental measurable quantity, the electron density
   fluctuation spectrum. Moreover, the validation and applicability of LES
   is demonstrated through a series of diagnostics based on the free
   energetics of the system. (C) 2014 AIP Publishing LLC.}},
