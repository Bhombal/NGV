@article{ ISI:000466708400024,
Author = {Bernard, T. N. and Shi, E. L. and Gentle, K. W. and Hakim, A. and
   Hammett, G. W. and Stoltzfus-Dueck, T. and Taylor, E. I.},
Title = {{Gyrokinetic continuum simulations of plasma turbulence in the Texas
   Helimak}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2019}},
Volume = {{26}},
Number = {{4}},
Month = {{APR}},
Abstract = {{The first gyrokinetic simulations of plasma turbulence in the Texas
   Helimak device, a simple magnetized torus, are presented. The device has
   features similar to the scrape-off layer region of tokamaks, such as
   bad-curvature-driven instabilities and sheath boundary conditions on the
   end plates, which are included in these simulations. Comparisons between
   simulations and measurements from the experiment show not only
   similarities, including equilibrium profiles and fluctuation amplitudes
   that approach experimental values, but also some important quantitative
   differences. Both experimental and simulation results exhibit turbulence
   statistics that are characteristic of blob transport. Published under
   license by AIP Publishing.}},
