@article{ ISI:A1993LL90000009,
Author = {KROMMES, JA},
Title = {{ADVANCES IN GYROKINETIC FLUCTUATION THEORY - THE GYROKINETIC FLUCTUATION
   DISSIPATION THEOREM AND DIELECTRIC FUNCTION}},
Journal = {{PHYSICS OF FLUIDS B-PLASMA PHYSICS}},
Year = {{1993}},
Volume = {{5}},
Number = {{7, 2}},
Pages = {{2405-2411}},
Month = {{JUL}},
Abstract = {{Thermal-equilibrium fluctuation spectra of finite-beta gyrokinetic (GK)
   plasmas are calculated by means of a GK fluctuation-dissipation theorem.
   Agreement is found with the earlier heuristic electrostatic calculations
   of Krommes et al. {[}Phys. Fluids 29, 2421 (1986)]. The formulas are
   interpreted in terms of the concept of a GK vacuum, whose large
   dielectric permittivity and magnetic permeability embody the shielding
   effects of the polarization drift and the parallel electric field. The
   finite-beta quasimode action density N(k) is calculated. This is used to
   interpret the result of a formal resonance integration over the normal
   modes in terms of the equipartition law omega(k)N(k) = T/2.}},
