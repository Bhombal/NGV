@inproceedings{ ISI:000167297600013,
Author = {Lee, WW},
Editor = {{Connor, JW and Sauter, O and Sindoni, E}},
Title = {{Shear-Alfven waves in gyrokinetic particle simulation}},
Booktitle = {{THEORY OF FUSION PLASMAS}},
