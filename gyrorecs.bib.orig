@article{ ISI:000259680600035,
Author = {Mikkelsen, D. R. and Dorland, W.},
Title = {{Dimits shift in realistic gyrokinetic plasma-turbulence simulations}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2008}},
Volume = {{101}},
Number = {{13}},
Month = {{SEP 26}},
Abstract = {{In simulations of turbulent plasma transport due to long wavelength (k
   perpendicular to p(i) <= 1) electrostatic drift-type instabilities, we
   find a persistent nonlinear up-shift of the effective threshold.
   Next-generation tokamaks will likely benefit from the higher effective
   threshold for turbulent transport, and transport models should
   incorporate suitable corrections to linear thresholds. The gyrokinetic
   simulations reported here are more realistic than previous reports of a
   Dimits shift because they include nonadiabatic electron dynamics, strong
   collisional damping of zonal flows, and finite electron and ion
   collisionality together with realistic shaped magnetic geometry.
   Reversing previously reported results based on idealized adiabatic
   electrons, we find that increasing collisionality reduces the heat flux
   because collisionality reduces the nonadiabatic electron
   microinstability drive.}},
