@article{ ISI:000245320000021,
Author = {Jenkins, Thomas G. and Lee, W. W.},
Title = {{Fluctuations and discrete particle noise in gyrokinetic simulation of
   drift waves}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2007}},
Volume = {{14}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{The relevance of the gyrokinetic fluctuation-dissipation theorem (FDT)
   to thermal equilibrium and nonequilibrium states of the gyrokinetic
   plasma is explored, with particular focus being given to the
   contribution of weakly damped normal modes to the fluctuation spectrum.
   It is found that the fluctuation energy carried in the normal modes
   exhibits the proper scaling with particle count (as predicted by the FDT
   in thermal equilibrium) even in the presence of drift waves, which grow
   linearly and attain a nonlinearly saturated steady state. This favorable
   scaling is preserved, and the saturation amplitude of the drift wave
   unaffected, for parameter regimes in which the normal modes become
   strongly damped and introduce a broad spectrum of discreteness-induced
   background noise in frequency space. (c) 2007 American Institute of
   Physics.}},
